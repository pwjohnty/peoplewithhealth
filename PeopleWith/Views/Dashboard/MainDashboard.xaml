<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Shell.NavBarHasShadow="False" Shell.NavBarIsVisible="False"
             xmlns:ios="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.iOSSpecific;assembly=Microsoft.Maui.Controls"
             ios:Page.UseSafeArea="True" 
             xmlns:data="clr-namespace:Syncfusion.Maui.DataSource;assembly=Syncfusion.Maui.DataSource"
             xmlns:syncfusion="clr-namespace:Syncfusion.Maui.ListView;assembly=Syncfusion.Maui.ListView"
             xmlns:chart="clr-namespace:Syncfusion.Maui.Charts;assembly=Syncfusion.Maui.Charts"
             xmlns:tabView="clr-namespace:Syncfusion.Maui.TabView;assembly=Syncfusion.Maui.TabView"
             xmlns:progressBar="clr-namespace:Syncfusion.Maui.ProgressBar;assembly=Syncfusion.Maui.ProgressBar"
             x:Class="PeopleWith.MainDashboard" BackgroundColor="White"
             >
    <ContentPage.Content>
        <tabView:SfTabView 
                     
                  IndicatorBackground="#031926"
         BackgroundColor="Transparent" TabItemTapped="tabsview_TabItemTapped"
                      IndicatorPlacement="Bottom" 
                            x:Name="tabsview" 
                           TabBarPlacement="Bottom"
              TabBarHeight="60"
                        
                        >
            <tabView:SfTabView.Margin>
                <OnPlatform x:TypeArguments="Thickness">
                    <On Platform="iOS" Value="0,0,0,0"/>
                    <On Platform="Android" Value="0,0,0,0"/>
                </OnPlatform>
            </tabView:SfTabView.Margin>

            <tabView:SfTabItem x:Name="hometab" Header="Home"  FontFamily="HankenGroteskBold" ImageSource="dashiconactive.png" FontSize="12" TextColor="#031926">
                <tabView:SfTabItem.Content>

                    <ScrollView BackgroundColor="Transparent" VerticalScrollBarVisibility="Never">
                        <StackLayout BackgroundColor="Transparent" Margin="0,20,0,0" IgnoreSafeArea="True">


                            <Grid Margin="20,0,20,0">

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="85*"/>
                                   
                                    <ColumnDefinition Width="15*"/>
                                </Grid.ColumnDefinitions>


                                <Image Source="pwlogo.png" HorizontalOptions="StartAndExpand" VerticalOptions="Center" HeightRequest="25" Grid.Column="0" Grid.Row="0" />

                          


                                <Image Source="usericon.png" HorizontalOptions="End" IsVisible="False" VerticalOptions="Center" HeightRequest="25"  Grid.Column="1" Grid.Row="0" />


                                <Frame Padding="0" HasShadow="False" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Margin="0,10,0,0" InputTransparent="False" BorderColor="Transparent" BackgroundColor="#e5f0fb" CornerRadius="{OnPlatform Android='10', iOS='10'}" >
                                    <Grid Padding="10">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="10*"/>
                                            <ColumnDefinition Width="80*"/>
                                            <ColumnDefinition Width="15*"/>
                                        </Grid.ColumnDefinitions>
                                        <Image Source="searchbaricon.png" HorizontalOptions="Start" HeightRequest="15" BackgroundColor="Transparent" WidthRequest="15"  Grid.Row="0" Grid.Column="0"  />
                                        <Label Text="Search for any PeopleWith help videos" TextColor="#031926" FontFamily="HankenGroteskRegular" FontSize="12" Grid.Row="0" Grid.Column="1" HorizontalOptions="Start" />

                                        <Grid.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_1"/>
                                        </Grid.GestureRecognizers>
                                    </Grid>
                                </Frame>

                                <!--<BoxView Color="#031926" HeightRequest="2" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" Margin="0,10,0,0"/>

                                <BoxView Color="#009fe3" HeightRequest="2" Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="2" Margin="0,10,0,0"/>-->

                            </Grid>


                            <syncfusion:SfListView x:Name="allhelpvideocatlist" ItemTapped="allhelpvideocatlist_ItemTapped" BackgroundColor="Transparent"  SelectionBackground="Transparent" ScrollBarVisibility="Never" Orientation="Horizontal" ItemSize="90" HeightRequest="100"     Margin="10,20,10,10" >
                                <syncfusion:SfListView.ItemTemplate>
                                    <DataTemplate>
                                        <ViewCell>
                                            <ViewCell.View>
                  
                                                    <Frame CornerRadius="10" BackgroundColor="{Binding BackgroundColor}" InputTransparent="True" BorderColor="Transparent" HasShadow="False" Padding="0" Margin="10,0,0,0">
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <!--<RowDefinition>
                                                    <RowDefinition.Height>
                                                        <OnPlatform x:TypeArguments="GridLength" Android="90" iOS="90"/>
                                                    </RowDefinition.Height>
                                                </RowDefinition>-->
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="100*"/>
                                                        </Grid.ColumnDefinitions>
                                                        <Frame CornerRadius="10" VerticalOptions="FillAndExpand" BackgroundColor="Transparent" InputTransparent="False" BorderColor="Transparent"   HorizontalOptions="FillAndExpand"  Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="1" Margin="0,0,0,0"  Padding="0" HasShadow="False"  IsClippedToBounds="True" >
                                                            <Image
                       Aspect="AspectFit"
                        BackgroundColor="Transparent"
                                          Margin="10"
                           Source="{Binding ContactImage}"
                        HorizontalOptions="CenterAndExpand"
                        VerticalOptions="FillAndExpand"
                        />
                                                        </Frame>
                                                        <Label VerticalOptions="CenterAndExpand" HorizontalOptions="Center" HorizontalTextAlignment="Center" Text="{Binding Title}" FontSize="10"  Margin="0,0,0,0"  TextColor="#031926" FontFamily="HankenGroteskBold"   Grid.Column="0" Grid.ColumnSpan="1" Grid.Row="1"/>
                                                        <!--<Frame CornerRadius="10" VerticalOptions="Start" BackgroundColor="White" BorderColor="White"   HorizontalOptions="FillAndExpand" HeightRequest="80" Margin="0,0,0,0" Grid.Column="0" Grid.ColumnSpan="1" Grid.Row="1"  Padding="0" HasShadow="False"  IsClippedToBounds="True" >
                       <StackLayout>
                           </StackLayout>
                       </Frame>-->
                                                    </Grid>
                                                </Frame>
                                            </ViewCell.View>
                                        </ViewCell>
                                    </DataTemplate>
                                </syncfusion:SfListView.ItemTemplate>
                            </syncfusion:SfListView>

                            <Label Text="For You" TextColor="#031926" FontFamily="HankenGroteskBold" FontSize="18" Margin="20,10,0,0" HorizontalOptions="Start" />


                            <StackLayout BackgroundColor="#e5f0fb" Margin="0,10,0,0">


                                <ActivityIndicator IsRunning="True" Margin="0,50,0,50" Color="#031926" VerticalOptions="Center" HorizontalOptions="Center" x:Name="listloader" IsVisible="True"/>

                                <syncfusion:SfListView x:Name="activitylist"  ItemTapped="activitylist_ItemTapped" IsVisible="False" BackgroundColor="Transparent"  SelectionBackground="Transparent" ScrollBarVisibility="Never" Orientation="Horizontal" ItemSize="250" HeightRequest="160"     Margin="0,10,0,10" >
                                    <syncfusion:SfListView.ItemsLayout>
                                        <syncfusion:GridLayout SpanCount="2" />
                                    </syncfusion:SfListView.ItemsLayout>
                                    <syncfusion:SfListView.ItemTemplate>
                                        <DataTemplate>
                                            <ViewCell>
                                                <ViewCell.View>
                                                    <Frame CornerRadius="10" BackgroundColor="White" InputTransparent="True"  BorderColor="Transparent" HasShadow="False" Padding="0" Margin="10,5,0,5">
                                                        <Grid>
                                                            <Grid.RowDefinitions>
                                                                <!--<RowDefinition>
                               <RowDefinition.Height>
                                   <OnPlatform x:TypeArguments="GridLength" Android="90" iOS="90"/>
                               </RowDefinition.Height>
                           </RowDefinition>-->
                                                                <RowDefinition Height="Auto"/>
                                                                <RowDefinition Height="Auto"/>
                                                            </Grid.RowDefinitions>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="25*"/>
                                                                <ColumnDefinition Width="75*"/>
                                                            </Grid.ColumnDefinitions>
                                                            <Frame CornerRadius="10" VerticalOptions="FillAndExpand" BackgroundColor="{Binding BackgroundColor}" BorderColor="Transparent"   HorizontalOptions="FillAndExpand"  Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="1" Margin="5,5,0,0"  Padding="0" HasShadow="False"  IsClippedToBounds="True" >
                                                                <Image
  Aspect="AspectFit"
   BackgroundColor="Transparent"
                     Margin="10" 
      Source="{Binding ContactImage}"
   HorizontalOptions="CenterAndExpand"
   VerticalOptions="FillAndExpand"
   />
                                                            </Frame>
                                                            <Label VerticalOptions="Start" HorizontalOptions="Start" HorizontalTextAlignment="Start" Text="{Binding Title} " FontSize="14"  Margin="10,10,0,0"  TextColor="#031926" FontFamily="HankenGroteskBold"   Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="0"/>
                                                            <Label VerticalOptions="Start" HorizontalOptions="Start" IsVisible="False" HorizontalTextAlignment="Start" Text="Symptoms" FontSize="10"  Margin="10,-5,0,0"  TextColor="#031926" FontFamily="HankenGroteskRegular"   Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="1"/>
                                                            <!--<Frame CornerRadius="10" VerticalOptions="Start" BackgroundColor="White" BorderColor="White"   HorizontalOptions="FillAndExpand" HeightRequest="80" Margin="0,0,0,0" Grid.Column="0" Grid.ColumnSpan="1" Grid.Row="1"  Padding="0" HasShadow="False"  IsClippedToBounds="True" >
  <StackLayout>
      </StackLayout>
  </Frame>-->
                                                        </Grid>
                                                    </Frame>
                                                </ViewCell.View>
                                            </ViewCell>
                                        </DataTemplate>
                                    </syncfusion:SfListView.ItemTemplate>
                                </syncfusion:SfListView>




                            </StackLayout>


           

                            <Frame BorderColor="Transparent" BackgroundColor="White" IsClippedToBounds="True" InputTransparent="True" Margin="20,20,20,0" Padding="0" HeightRequest="120" HasShadow="False" CornerRadius="10">

                                <Image Aspect="AspectFill" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand" x:Name="maindashimage"/>

                            </Frame>




                            <Label Text="Daily Tasks" TextColor="#031926" FontFamily="HankenGroteskBold" FontSize="18" Margin="20,20,0,0" HorizontalOptions="Start" />

                            <StackLayout BackgroundColor="#f9f9fb" Margin="0,10,0,0">

                                                          

                            <Grid InputTransparent="False" BackgroundColor="White" Padding="5" Margin="0,10,0,10">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="30*"/>
                                            <ColumnDefinition Width="5*"/>
                                            <ColumnDefinition Width="30*"/>
                                            <ColumnDefinition Width="5*"/>
                                            <ColumnDefinition Width="30*"/>
                                        </Grid.ColumnDefinitions>

                                      
                                        <StackLayout Grid.Row="0" Grid.Column="0" Grid.RowSpan="3">
                                        <Label Text="Medications Due" TextColor="#031926" Grid.Row="0" Grid.Column="0" FontFamily="HankenGroteskBold" FontSize="10" Margin="0,0,0,10" VerticalOptions="Start" HorizontalOptions="Center" />

                                        <progressBar:SfCircularProgressBar x:Name="medprogressbar"
                                   HorizontalOptions="Center" TrackFill="#e5f9f4" ProgressFill="Teal"
                                   ProgressThickness="10" AnimationEasing="{x:Static Easing.CubicInOut}" 
                                   HeightRequest="100" WidthRequest="100"
                                   TrackThickness="10"
                                   TrackCornerStyle="BothCurve"
                                   ProgressCornerStyle="BothCurve"
                                   BackgroundColor="Transparent"
                                   Margin="0" Maximum="100"
                                   Grid.Column="0"
                                   Grid.ColumnSpan="1"
                                   Grid.Row="1">

                                            
                                            <progressBar:SfCircularProgressBar.Content>
                                                <Grid VerticalOptions="Center" HorizontalOptions="Center">
                                                    <Label Text="{Binding Source={x:Reference medprogressbar}, Path=Progress, StringFormat='{0:0}%'}"
                   TextColor="Teal" Padding="0" WidthRequest="100" HeightRequest="100" Background="Transparent"
                   FontFamily="HankenGroteskBold"
                   HorizontalTextAlignment="Center"
                   VerticalTextAlignment="Center"
                   FontSize="12" />
                                                </Grid>
                                            </progressBar:SfCircularProgressBar.Content>
                                        </progressBar:SfCircularProgressBar>

                                        <Label x:Name="MedsRemain" Margin="10,0,10,0"
                                                Grid.Column="0" Grid.ColumnSpan="1" Grid.Row="2"
TextColor="Teal" 
FontFamily="HankenGroteskBold"
HorizontalTextAlignment="Center"
VerticalTextAlignment="Center"
FontSize="8" />
                                            <StackLayout.GestureRecognizers>
                                                <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_5" NumberOfTapsRequired="1"/>
                                            </StackLayout.GestureRecognizers>

                                        </StackLayout>
                                        <BoxView BackgroundColor="#e5f0fb" Margin="20,5,20,5" VerticalOptions="FillAndExpand" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="1" WidthRequest="1" />

                                     
                                        <StackLayout Grid.Row="0" Grid.Column="2" Grid.RowSpan="3">
                                        <Label Text="Supplements Due" TextColor="#031926" Grid.Row="0" Grid.Column="2" FontFamily="HankenGroteskBold" FontSize="10" Margin="0,0,0,10" VerticalOptions="Start" HorizontalOptions="Center" />

                                        <progressBar:SfCircularProgressBar x:Name="suppprogressbar" 
                                    HorizontalOptions="Center" TrackFill="#f9f4e5" ProgressFill="Brown"  
                                    ProgressThickness="10" AnimationEasing="{x:Static Easing.CubicInOut}" 
                                    HeightRequest="100" WidthRequest="100"
                                    TrackThickness="10"
                                    TrackCornerStyle="BothCurve"
                                    ProgressCornerStyle="BothCurve"
                                    BackgroundColor="Transparent"
                                    Margin="0" Maximum="100"
                                    Grid.Column="2"
                                    Grid.ColumnSpan="1"
                                    Grid.Row="1">

                                            
                                            <progressBar:SfCircularProgressBar.Content>
                                                <Grid VerticalOptions="Center" HorizontalOptions="Center">
                                                    <Label Text="{Binding Source={x:Reference suppprogressbar}, Path=Progress, StringFormat='{0:0}%'}" 
                      TextColor="Brown" Padding="0" WidthRequest="100" HeightRequest="100" Background="Transparent"
                      FontFamily="HankenGroteskBold"
                      HorizontalTextAlignment="Center"
                      VerticalTextAlignment="Center"
                      FontSize="12" />

                                                </Grid>
                                            </progressBar:SfCircularProgressBar.Content>
                                        </progressBar:SfCircularProgressBar>


                                        <Label x:Name="SuppsRemain" Margin="10,0,10,0"
                                                Grid.Column="2" Grid.ColumnSpan="1" Grid.Row="2"
TextColor="Brown" 
FontFamily="HankenGroteskBold"
HorizontalTextAlignment="Center"
VerticalTextAlignment="Center"
FontSize="8" />
                                            <StackLayout.GestureRecognizers>
                                                <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_5" NumberOfTapsRequired="1"/>
                                            </StackLayout.GestureRecognizers>

                                        </StackLayout>


                                        <BoxView BackgroundColor="#e5f0fb" Margin="20,5,20,5" VerticalOptions="FillAndExpand" Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="1" WidthRequest="1" />


                                        <StackLayout Grid.Row="0" Grid.Column="4" Grid.RowSpan="3">
                                            <Label Text="Symptoms Updated" TextColor="#031926" Grid.Row="0" Grid.Column="4" FontFamily="HankenGroteskBold" FontSize="10" Margin="0,0,0,10" VerticalOptions="Start" HorizontalOptions="Center" />

                                            <progressBar:SfCircularProgressBar x:Name="symprogressbar" 
                                     HorizontalOptions="Center" TrackFill="#fff7ea" ProgressFill="Orange"
                                     ProgressThickness="10" AnimationEasing="{x:Static Easing.CubicInOut}" 
                                     HeightRequest="100" WidthRequest="100"
                                     TrackThickness="10"
                                     TrackCornerStyle="BothCurve"
                                     ProgressCornerStyle="BothCurve"
                                     BackgroundColor="Transparent"
                                     Margin="0" Maximum="100"
                                     Grid.Column="4"    
                                     Grid.ColumnSpan="2"
                                     Grid.Row="1">

                                                <progressBar:SfCircularProgressBar.Content>
                                                    <Grid VerticalOptions="Center" HorizontalOptions="Center" >
                                                        <Label Text="{Binding Source={x:Reference symprogressbar},Path=Progress, StringFormat='{0:0}%'}"
                        TextColor="Orange" Padding="0" WidthRequest="100" HeightRequest="100" Background="Transparent"
                        FontFamily="HankenGroteskBold"
                        HorizontalTextAlignment="Center"
                        VerticalTextAlignment="Center"
                        FontSize="12" />

                                                    </Grid>
                                                </progressBar:SfCircularProgressBar.Content>
                                            </progressBar:SfCircularProgressBar>

                                            <Label x:Name="SympRemain" Margin="10,0,10,0"
                                                Grid.Column="4" Grid.ColumnSpan="1" Grid.Row="2"
TextColor="Orange" 
FontFamily="HankenGroteskBold"
HorizontalTextAlignment="Center"
VerticalTextAlignment="Center"
FontSize="8" />

                                            <StackLayout.GestureRecognizers>
                                                <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_4" NumberOfTapsRequired="1"/>
                                            </StackLayout.GestureRecognizers>
                                        </StackLayout>

                                    </Grid>
                       
                            </StackLayout>



                            <Label Text="Your Recent Health" x:Name="RecentHealthlbl" TextColor="#031926" FontFamily="HankenGroteskBold" FontSize="18" Margin="20,20,0,0" HorizontalOptions="Start" />



                            <StackLayout  BackgroundColor="#f9f9fb" Margin="0,10,0,0">

                                <Label Text="Recent Symptom Updates" x:Name="recentsymlbl" TextColor="#031926" FontFamily="HankenGroteskBold" FontSize="12" Margin="15,10,0,0" HorizontalOptions="Start" />
                                
                                <syncfusion:SfListView x:Name="symptomdetaillist" ItemTapped="symptomdetaillist_ItemTapped" BackgroundColor="Transparent"  SelectionBackground="Transparent" ItemSize="250" HeightRequest="165" ScrollBarVisibility="Never" Orientation="Horizontal"  Margin="0,0,0,10" >
                                    <!--<syncfusion:SfListView.DataSource>
                                        <data:DataSource>
                                            <data:DataSource.GroupDescriptors>
                                                <data:GroupDescriptor PropertyName="label"/>
                                            </data:DataSource.GroupDescriptors>
                                        </data:DataSource>
                                    </syncfusion:SfListView.DataSource>
                                    <syncfusion:SfListView.GroupHeaderTemplate>
                                        <DataTemplate>
                                            <ViewCell>
                                                <StackLayout BackgroundColor="Transparent" Padding="10">
                                                    <Label Text="{Binding Key}" FontSize="14" Margin="10,0,0,0" FontFamily="HankenGroteskBold" TextColor="#031926" />
                                                </StackLayout>
                                            </ViewCell>
                                        </DataTemplate>
                                    </syncfusion:SfListView.GroupHeaderTemplate>-->
                                    <syncfusion:SfListView.ItemTemplate>
                                        <DataTemplate>
                                            <ViewCell>
                                                <ViewCell.View>
                                                    <Frame CornerRadius="10" BackgroundColor="White" InputTransparent="True" BorderColor="Transparent" HasShadow="False" Padding="0" Margin="10,5,0,5">
                                                        <Grid>
                                                            <Grid.RowDefinitions>
                                                                <!--<RowDefinition>
                             <RowDefinition.Height>
                                 <OnPlatform x:TypeArguments="GridLength" Android="90" iOS="90"/>
                             </RowDefinition.Height>
                         </RowDefinition>-->
                                                                <RowDefinition Height="Auto"/>
                                                                <RowDefinition Height="Auto"/>
                                                                <RowDefinition Height="Auto"/>
                                                                <RowDefinition Height="Auto"/>
                                                                <RowDefinition Height="Auto"/>
                                                                <RowDefinition Height="Auto"/>
                                                            </Grid.RowDefinitions>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="33*"/>
                                                                <ColumnDefinition Width="33*"/>
                                                                <ColumnDefinition Width="33*"/>
                                                   
                                                            </Grid.ColumnDefinitions>
                                     
                                                            <Label VerticalOptions="Start" HorizontalOptions="Start" HorizontalTextAlignment="Start" Text="{Binding label}" FontSize="14"  Margin="10,10,0,0"  TextColor="#031926" FontFamily="HankenGroteskBold"   Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="0"/>
                                                            <Label VerticalOptions="Start" HorizontalOptions="Start" HorizontalTextAlignment="Start" Text="{Binding datetimestring}" FontSize="12"  Margin="10,0,0,0"  TextColor="Gray" FontFamily="HankenGroteskRegular"   Grid.Column="0" Grid.ColumnSpan="3" Grid.Row="1"/>

                                                            <Label VerticalOptions="Center" HorizontalOptions="Start" HorizontalTextAlignment="Start" Text="{Binding value}" FontSize="26"  Margin="20,0,0,0"  TextColor="Orange" FontFamily="HankenGroteskBold"   Grid.Column="3" Grid.ColumnSpan="1" Grid.Row="0" Grid.RowSpan="2"/>

                                                            <Label VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Start" Text="Average" FontSize="10"  Margin="0,10,0,0"  TextColor="Gray" FontFamily="HankenGroteskBold"   Grid.Column="0" Grid.ColumnSpan="1" Grid.Row="2"/>
                                                            <Label VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Start" Text="Highest" FontSize="10"  Margin="0,10,0,0"  TextColor="Gray" FontFamily="HankenGroteskBold"   Grid.Column="1" Grid.ColumnSpan="1" Grid.Row="2"/>
                                                            <Label VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Start" Text="No of Times" FontSize="10"  Margin="0,10,0,0"  TextColor="Gray" FontFamily="HankenGroteskBold"   Grid.Column="2" Grid.ColumnSpan="1" Grid.Row="2"/>

                                                            <Label VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Start" Text="{Binding avgstring}" FontSize="18"  Margin="0,0,0,0"  TextColor="#031926" FontFamily="HankenGroteskBold"   Grid.Column="0" Grid.ColumnSpan="1" Grid.Row="3"/>
                                                            <Label VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Start" Text="{Binding highstring}" FontSize="18"  Margin="0,0,0,0"  TextColor="#031926" FontFamily="HankenGroteskBold"   Grid.Column="1" Grid.ColumnSpan="1" Grid.Row="3"/>
                                                            <Label VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Start" Text="{Binding nooftimesstring}" FontSize="18"  Margin="0,0,0,0"  TextColor="#031926" FontFamily="HankenGroteskBold"   Grid.Column="2" Grid.ColumnSpan="1" Grid.Row="3"/>

                                                            <Button Text="Update" BorderWidth="2" Padding="10" Margin="10" TextColor="Orange" FontFamily="HankenGroteskBold" Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="3" HorizontalOptions="FillAndExpand" VerticalOptions="Fill" BorderColor="Orange" BackgroundColor="Transparent" /> 


                                                            <Image Source="{Binding img}" IsVisible="False" Margin="0,10,0,0" VerticalOptions="Center" Grid.Column="0" Grid.ColumnSpan="1" HorizontalOptions="End" Grid.Row="4" HeightRequest="20" WidthRequest="20" />
                                                            <Label VerticalOptions="Center" IsVisible="False" HorizontalOptions="Start" HorizontalTextAlignment="Start" Text="{Binding improvelbl}" FontSize="14"  Margin="10,10,0,0"  TextColor="Orange" FontFamily="HankenGroteskBold"   Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="4"/>


                                                            <Frame CornerRadius="10" IsVisible="False" BackgroundColor="#fff7ea" BorderColor="Transparent" Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="3" HeightRequest="50" HasShadow="False" Padding="0" Margin="5,10,5,5">
                                                                <Label VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" Text="{Binding symptominfo}" FontSize="12"  Margin="5,0,5,0"  TextColor="#031926" FontFamily="HankenGroteskRegular" />
                                                            </Frame>

                                                            <!--<chart:SfCartesianChart Margin="-5,0,0,10" BackgroundColor="Transparent" Grid.Column="0" Grid.ColumnSpan="3" Grid.Row="5" x:Name="SymptomProgChart">
                                                                <chart:SfCartesianChart.HeightRequest>
                                                                    <OnPlatform x:TypeArguments="x:Double">
                                                                        <On Platform="iOS" Value="80" />

                                                </Grid.ColumnDefinitions>

                                                <Label Text="{Binding label}" FontFamily="HankenGroteskRegular" Margin="0,0,0,0"  Grid.Row="0" Grid.Column="0" VerticalOptions="StartAndExpand"  TextColor="Gray" FontSize="12" />



                                            </Grid>
                                        </Frame>
                                    </ViewCell>
